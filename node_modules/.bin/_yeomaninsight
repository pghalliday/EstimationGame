#!/bin/sh
basedir=`dirname "$0"`

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/python" ]; then
  "$basedir/python"  "$basedir/../yeoman/bin/yeomaninsight.py" "$@"
  ret=$?
else 
  python  "$basedir/../yeoman/bin/yeomaninsight.py" "$@"
  ret=$?
fi
exit $ret
